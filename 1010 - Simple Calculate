import java.util.*

fun main(args: Array<String>) {

val(code1, quantity1, price1) = readLine()!!.split(" ").map { it.toDouble() }
    val(code2, quantity2, price2) = readLine()!!.split(" ").map { it.toDouble() }

    val total = (quantity1 * price1) + (quantity2 * price2)

    println("VALOR A PAGAR: R$ %.2f".format(total))
	
}
