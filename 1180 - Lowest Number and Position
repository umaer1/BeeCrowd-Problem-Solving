#include <stdio.h>
 
int main() {
 
    int n,i;
    int min,pos;
    
    scanf("%d",&n);
    
    int array[n];
    for(i=0; i<n;i++) {
        scanf("%d",&array[i]);
    }
    
    min = array[0];
   
    for(i=1; i<n; i++) {
      if(min>array[i]) {
          min = array[i];
          pos = i;
      } 
      
    }
    printf("Menor valor: %d\n",min);
    printf("Posicao: %d\n",pos);
 
    return 0;
}
