import java.util.*

fun main(args: Array<String>) {

	val scanner = Scanner(System.`in`)
    val NC = scanner.nextInt()

    for (case in 1..NC) {
        val n = scanner.nextInt()
        val k = scanner.nextInt()

        var k1 = 0
        for (i in 1..n) {
            k1 = (k1 + k) % i
        }

        println("Case $case: ${k1 + 1}")
    }
	
}
