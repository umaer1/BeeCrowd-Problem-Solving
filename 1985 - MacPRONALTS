#include<stdio.h>
int main()
{
    float array[5] = {1.50,2.50,3.50,4.50,5.50};
    int i,p,q,n;
    float tot=0;



    while(scanf("%d",&n)!= EOF) {
        
        for(i=0; i<n; i++){
          scanf("%d %d",&p,&q);
          p = p%1000;
          tot = tot+ (q*array[p-1]);
        }
        printf("%.2f\n",tot);
    }


    return 0;
}
