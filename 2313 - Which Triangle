#include<stdio.h>
int main() {
    
    int A,B,C;
    scanf("%d %d %d",&A,&B,&C);
    
    
    
    if(A+C > B) {
        if(A==B && B==C && A==C) {
            printf("Valido-Equilatero\n");
        }
        
        else if(A==B || A==C || B==C) {
            printf("Valido-Isoceles\n");
        }
        
        else if(A!=B && B!=C && C!=A) {
          printf("Valido-Escaleno\n");
        }
        
        if(A*A == (B*B + C*C ) || B*B == (A*A + C*C)  ||C*C == (B*B + A*A) ) {
    printf("Retangulo: S\n");
  }
        else {
           printf("Retangulo: N\n");
        }
        
    }

    else {
          printf("Invalido\n");
    }
    return 0;
}
